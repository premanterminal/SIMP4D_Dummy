<?xml version="1.0" encoding="UTF-8"?>
<?eclipse version="3.4"?>
<plugin>

   <extension
         id="application"
         point="org.eclipse.core.runtime.applications">
      <application>
         <run
               class="com.dispenda.main.Application">
         </run>
      </application>
   </extension>
   <extension
         point="org.eclipse.ui.perspectives">
      <perspective
            name="Main Perspective"
            class="com.dispenda.perspectives.MainPerspective"
            id="com.dispenda.perspectives.MainPerspective">
      </perspective>
   </extension>
   <extension
         point="org.eclipse.ui.views">
      <view
            allowMultiple="false"
            class="com.dispenda.views.MainView"
            icon="img/home.gif"
            id="com.dispenda.views.MainView"
            name="Menu Utama">
      </view>
      <view
            allowMultiple="false"
            class="com.dispenda.views.ToolbarView"
            icon="img/home.gif"
            id="com.dispenda.views.ToolbarView"
            name="Toolbar">
      </view>
   </extension>
   <extension
         id="product"
         point="org.eclipse.core.runtime.products">
      <product
            application="com.dispenda.main.application"
            name="Dispenda 2015">
         <property
               name="aboutText"
               value="Created by CV. Mataniari&#x0A;Copyright Dinas Pendapatan Daerah Kota Medan 2015">
         </property>
         <property
               name="windowImages"
               value="icons/logo16x16.gif,icons/logo32x32.gif,icons/logo48x48.gif,icons/logo64x64.gif,icons/logo128x128.gif">
         </property>
         <property
               name="aboutImage"
               value="product_lg.gif">
         </property>
         <property
               name="appName"
               value="Dispenda 2015">
         </property>
         <property
               name="preferenceCustomization"
               value="plugin_customization.ini">
         </property>
      </product>
   </extension>
   <extension
         point="org.eclipse.ui.menus">
   </extension>
   <extension
         point="org.eclipse.ui.splashHandlers">
      <splashHandler
            class="com.dispenda.main.splashHandlers.InteractiveSplashHandler"
            id="com.dispenda.main.splashHandlers.interactive">
      </splashHandler>
   </extension>

</plugin>
