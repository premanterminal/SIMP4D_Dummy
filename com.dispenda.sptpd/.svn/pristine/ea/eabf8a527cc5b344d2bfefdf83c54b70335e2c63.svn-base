<?xml version="1.0" encoding="UTF-8"?>
<?eclipse version="3.4"?>
<plugin>
   <extension
         point="org.eclipse.ui.views">
      <category
            name="SPTPD Category"
            id="com.dispenda.sptpd">
      </category>
      <view
            category="com.dispenda.sptpd"
            class="com.dispenda.sptpd.views.InputSPTPDView"
            icon="icons/sptpd.gif"
            id="com.dispenda.sptpd.views.InputSPTPDView"
            name="Input SPTPD">
      </view>
      <view
            category="com.dispenda.sptpd"
            class="com.dispenda.sptpd.views.DaftarSPTPDView"
            icon="icons/sptpd.gif"
            id="com.dispenda.sptpd.views.DaftarSPTPDView"
            name="Daftar SPTPD">
      </view>
      <view
            category="com.dispenda.sptpd"
            class="com.dispenda.sptpd.views.DaftarSKPDView"
            icon="icons/sptpd.gif"
            id="com.dispenda.sptpd.views.DaftarSKPDView"
            name="Daftar SKPD">
      </view>
      <view
            category="com.dispenda.sptpd"
            class="com.dispenda.sptpd.views.DaftarSKPDKeuanganView"
            icon="icons/sptpd.gif"
            id="com.dispenda.sptpd.views.DaftarSKPDKeuanganView"
            name="Daftar SKPD Keuangan">
      </view>
   </extension>
   <extension
         point="org.eclipse.ui.perspectiveExtensions">
      <perspectiveExtension
            targetID="org.eclipse.jdt.ui.JavaPerspective">
         <view
               ratio="0.5"
               relative="org.eclipse.ui.views.TaskList"
               relationship="right"
               id="com.dispenda.sptpd.views.SampleView">
         </view>
      </perspectiveExtension>
   </extension>

</plugin>
